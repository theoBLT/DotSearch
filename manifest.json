{
 "manifest_version": 2,
 "name": "Rabbit search ⚡️",
 "description": "Use short keywords to access all of your company's universe in a second.⚡️",
 "version": "4",
 "author": "Théo Blochet",
 "browser_action": {
  "default_title": "Update your shortcuts! ⚡️"
 },
 "options_ui": {
  "page": "setup/setup.html",
  "open_in_tab": false
},
 "omnibox": { "keyword" : "qq" },
 "icons": {
   "16": "16-full-color.png",
   "48": "48-full-color.png",
   "128":"128-full-color.png"
 },
 "background": {
   "scripts": ["background.js"],
   "persistent": false
 },
 "permissions": ["activeTab","storage"]
}

